<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        Returned Order #<%= @admin_order.id %> 
        <%#= link_to '<i class="fa fa-plus"></i> Add New client'.html_safe, new_admin_product_path, class: "btn btn-default pull-right add-btn" %>
      </div>
      <div class="panel-body">
        <div class="dataTable_wrapper">
          <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Client Name</th>
                    <th>Cargo Charges</th>
                    <th>Grand Total</th>
                    <th>Date</th>
                    
                </tr>
            </thead>
            <tbody>

                <tr class="odd gradeX">
                  <!-- <td><%#= index+1 %></td> -->
                  <td><%= @admin_order.id %></td>
                  <td><%= @admin_order.client.name %></td>
                  <td><%= @admin_order.shipping_charges.to_i %></td>
                  <td><%= @admin_order.grand_total %></td>
                  <td><%= @admin_order.created_at.strftime("%B-%d-%Y") %></td>

                </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>